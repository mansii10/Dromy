//Find the first occurance of the element in givin array (two array find the position of the element in array)

var array = [1, 2, 3, 4, 6, 3, 8, 3, 5, 7];

function findElement(arr1, element) {
    for (var i=0; i< arr1.length; i++) 
    {
        if (arr1[i]==element)
        {
            return i;
        }
    }
}

console.log(findElement(array, 3));
