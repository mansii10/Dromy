//Pop, Push, Peek Stack

function Stack() {
    this.head = null;
}
var sp = Stack.prototype;

sp.push = function(data) {
    var node = new ListElement(null, data);

    if (this.head) {
        node.next = this.head;
        this.head = node;

        return;
    }

    this.head = node;
};

sp.pop = function() {
    if (this.head) {
        var tmp = this.head;

        this.head = this.head.next;

        return tmp.data;
    }

    return null;
}

sp.peek = function() {
    if (this.head) {
        return this.head.data;
    }

    return null;
};

function ListElement(next, data) {
    this.next = next;
    this.data = data;
}


var stack = new Stack();
stack.push(4);
stack.push(5);
stack.push(3);

console.log(stack.peek());
console.log(stack.pop());
console.log(stack.pop());
console.log(stack.pop());

 
